{
	"$schema": "https://json.schemastore.org/prettierrc.json",
	"printWidth": 120,
	"tabWidth": 4,
	"useTabs": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"experimentalTernaries": true,
	"semi": true,
	"singleQuote": true,
	"proseWrap": "always",
	"htmlWhitespaceSensitivity": "css",
	"jsxSingleQuote": false,
	"bracketSameLine": false
}
