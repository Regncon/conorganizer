package components

templ CallToAction() {
	<div
		style="
            background-color: black;
            padding: 1rem;
        "
	>
		<h1>Vil du arrangere noe under Regncon?</h1>
		<p>
			Vi har åpnet dørene for registrering av arrangementer.
			Har du noe du  ønsker å ta med deg til årets festivalhelg?
		</p>
		<a
			href="/my-events"
			href="/auth"
			class="btn btn--cta"
			style="place-self: center end; margin-right:  1rem"
		>
			Send inn arrangement
		</a>
	</div>
}
